<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pkinc.begusaraisabji">

    <application
        android:allowBackup="true"
        android:icon="@drawable/veg_home"
        android:label="Begusarai Sabji"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.pkinc.begusaraisabji.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.pkinc.begusaraisabji.MainActivity" />
        <activity android:name="com.pkinc.begusaraisabji.MakeOrder" />
        <activity android:name="com.pkinc.begusaraisabji.Products" />
        <activity android:name="com.pkinc.begusaraisabji.VegPriceList" />
        <activity android:name="com.pkinc.begusaraisabji.FruitPriceList" />
        <activity android:name="com.pkinc.begusaraisabji.Us"></activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8106090973347908~1512187484" />

    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

</manifest>